    {
      "model": "gemini-1.5-pro",
      "overrideScope": "creative-writing",
      "temperature": 0.9
    }
  ],
  "tools": {
    "sandbox": true,
    "autoAccept": false
  },
  "folderTrust": {
    "enabled": true
  }
}
