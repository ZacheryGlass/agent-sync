{
  "customAliases": {
    "fast": {
      "model": "gemini-2.0-flash",
      "temperature": 0.7,
      "maxOutputTokens": 8192,
      "topP": 0.95
    },
    "precise": {
      "model": "gemini-2.0-flash",
      "temperature": 0.2,
      "maxOutputTokens": 4096,
      "topP": 0.9
    },
    "creative": {
      "model": "gemini-1.5-pro",
      "temperature": 1.0,
      "maxOutputTokens": 8192,
      "topP": 0.95
    },
    "thinking": {
      "model": "gemini-2.0-flash-thinking-exp",
      "temperature": 0.5,
      "maxOutputTokens": 8192,
      "thinkingConfig": {
        "thinkingBudget": 2048,
        "includeThoughts": true
      }
    },
    "code-reviewer": {
      "extends": "precise",
      "maxOutputTokens": 8192
    },
    "documentation-writer": {
      "extends": "fast",
      "temperature": 0.5
